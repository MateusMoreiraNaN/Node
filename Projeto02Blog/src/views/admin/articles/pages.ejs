<%- include('../../partials/header.ejs'); -%>
<%- include('../../partials/homeNavbar.ejs'); -%>
<body>
    
    <div class="container">
        <br>
        <% result.articles.rows.forEach(article =>{ %>
            <div class="card">
                <div class="card-header">
                    <h2><%= article.title %></h2>
                </div>
                <div class="card-body">
                    <a href="/<%= article.slug %>" class="btn btn-success">Ler artigo</a>
                </div>
            </div>
        <% }) %>
        
    </div>
</body>
